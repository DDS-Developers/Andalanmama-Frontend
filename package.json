  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "ios": "react-native run-ios --simulator 'iPhone 6'",
    "android": "react-native run-android",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook-build": "NODE_ENV=production build-storybook -c .storybook -o storybook-build",
    "ssr": "rm -rf build && mkdir build && NODE_TARGET=ssr NODE_ENV=development webpack --content-base build/  --progress && mv ./build/index.html ./build/main.html && NODE_ENV=development node build/server.js",
    "ssr-build": "rm -rf build && mkdir build && NODE_TARGET=ssr NODE_ENV=production PLATFORM_ENV=web webpack --content-base build/  --progress && mv ./build/index.html ./build/main.html",
    "web": "cp ./src/web/templates/*.png ./build/ && NODE_ENV=development webpack-dev-server --content-base build/ --mode development --compress --port 3000 --inline --hot --colors --progress --history-api-fallback",
    "web-start": "NODE_ENV=development webpack-dev-server --content-base build/ --mode development --compress --port 3000 --inline --hot --colors --progress --history-api-fallback",
    "web-build": "rm -rf build && mkdir build && cp ./src/web/templates/*.png ./build/ && NODE_ENV=production PLATFORM_ENV=web webpack --content-base build/ --progress",
    "linting-native": "NODE_TARGET=native eslint src/native/",
    "linting-native-fix": "NODE_TARGET=native eslint src/native/ --fix",
    "linting-web": "eslint src/web/",
    "postversion": "react-native-version --increment-build",
    "android-bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build-apk": "npm run android-bundle && cd android && sudo ./gradlew assembleDebug"
  },
